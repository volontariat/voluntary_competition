<% if @season.active? %>
<div id="tournament_bracket">
  <%= render partial: 'competition/tournament_seasons/bracket' %>
</div>
  <% content_for :javascript_includes do %>
    <%= javascript_include_tag 'voluntary_competition/seasons/bracket' %>
  <% end %>
<% else %>
  <p><%= I18n.t('tournament_seasons.bracket.unavailable')%></p>
<% end %>